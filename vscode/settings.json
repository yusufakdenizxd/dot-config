{
  "css.lint.unknownAtRules": "ignore",
  "workbench.iconTheme": "material-icon-theme",
  "editor.formatOnSave": false,
  "editor.unicodeHighlight.allowedLocales": {
    "tr": true
  },
  "search.exclude": {
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "explorer.confirmDragAndDrop": false,
  "dart.showDevToolsDebugToolBarButtons": false,
  "debug.toolBarLocation": "docked",
  "git.confirmSync": false,
  "workbench.editor.showTabs": "multiple",
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "last",
  "breadcrumbs.icons": true,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showModules": false,
  "material-icon-theme.folders.associations": {
    "repositories": "Controller",
    "repository": "Controller",
    "features": "Context",
    "feature": "Context",
    "presentation": "Review",
    "mixins": "Controller",
    "notifier": "Components"
  },
  "explorer.fileNesting.enabled": true,

  "explorer.fileNesting.patterns": {
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,analysis_options.yaml,.gitignore,README.md,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
    "*.dart": "${capture}.g.dart"
  },

  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },

    {
      "tag": "FIXME",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  //VIM

  "background.enabled": true,
  "background.fullscreen": {
    "images": [
      "file:///Users/yusufakdeniz/Pictures/wallpaper/wallhaven-6d59xl_3840x2160.png"
    ],
    "opacity": 0.88, // 0.85 ~ 0.95 recommended
    "size": "cover", // also css, `cover` to self-adaption (recommended)，or `contain`、`200px 200px`
    "position": "center", // alias to `background-position`, default `center`
    "interval": 0 // seconds of interval for carousel, default `0` to disabled.
  },

  "vim.highlightedyank.duration": 200,
  "vim.highlightedyank.enable": true,
  "vim.statusBarColorControl": true,

  "vim.statusBarColors.normal": ["#56B6C2"],
  "vim.statusBarColors.replace": "#FF0000",
  "vim.statusBarColors.insert": "#61AFEF",
  "vim.statusBarColors.visual": "#A3BE8C",
  "vim.statusBarColors.visualline": "#A3BE8C",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.commandlineinprogress": "#61AFEF",
  "vim.statusBarColors.searchinprogressmode": "#ABB2BF",
  "vim.statusBarColors.easymotionmode": "#ABB2BF",
  "vim.statusBarColors.easymotioninputmode": "#ABB2BF",

  // "vim.statusBarColors.replace": ["#56B6C2"],
  // "vim.statusBarColors.normal": "#FF0000",
  // "vim.statusBarColors.insert": "#FF3232",
  // "vim.statusBarColors.visual": "#A3BE8C",
  // "vim.statusBarColors.visualline": "#A3BE8C",
  // "vim.statusBarColors.visualblock": "#A3BE8C",
  // "vim.statusBarColors.commandlineinprogress": "#61AFEF",
  // "vim.statusBarColors.searchinprogressmode": "#ABB2BF",
  // "vim.statusBarColors.easymotionmode": "#ABB2BF",
  // "vim.statusBarColors.easymotioninputmode": "#ABB2BF",

  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.leader": " ",
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },

    {
      "before": ["<leader>", "c"],
      "after": ["\"", "_", "c"]
    },
    {
      "before": ["q"],
      "commands": ["editor.action.quickFix"]
    }
  ],
  "vim.insertModeKeyBindings": [],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "2", "s"]
    },
    {
      "before": ["<cr>"],
      "after": [":"]
    },
    {
      "before": [","],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<leader>", "<leader>", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<leader>", "<leader>", "k"]
    },

    {
      "before": ["<C-l>"],
      "after": ["<leader>", "<leader>", "l"]
    },
    {
      "before": ["<C-h>"],
      "after": ["<leader>", "<leader>", "h"]
    }
  ],

  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-q>"],
      "after": []
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "f"],
      "after": []
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["editor.action.openLink"]
    },
    // {
    //   "before": ["<leader>", "v"],
    //   "commands": ["markdown-preview-enhanced.openPreviewToTheSide"]
    // },
    {
      "before": ["q"],
      "commands": ["editor.action.quickFix"]
    },

    {
      "before": ["Q"],
      "commands": ["editor.action.rename"]
    },

    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.showHover"]
    },

    {
      "before": ["J"],
      "after": ["g", "j"]
    },

    {
      "before": ["K"],
      "after": ["g", "k"]
    },

    {
      "before": ["<leader>", "w"],
      "commands": [
        "workbench.action.splitEditor",
        "workbench.action.focusLeftGroup",
        "workbench.action.closeActiveEditor",
        "workbench.action.focusRightGroup"
      ]
    },
    {
      "before": ["<leader>", "s"],

      "commands": [
        "workbench.action.splitEditorDown",
      ]
    },

    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },

    {
      "before": ["<leader>", "c"],
      "after": ["\"", "_", "c"]
    }
  ],
  //END VIM

  //NEOVIM
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "$HOME/.config/nvim/init.lua",
  //END NEOVIM

  "dart.warnWhenEditingFilesInPubCache": false,
  "dart.lineLength": 100000,
  "editor.fontFamily": "CaskaydiaCove Nerd Font Mono",
  // "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "dart.runPubGetOnPubspecChanges": "never",
  "editor.suggest.matchOnWordStartOnly": false,
  "explorer.confirmDelete": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "off",
    "editor.wordBasedSuggestions": "off",
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    }
  },
  "workbench.startupEditor": "none",
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "editor.tabSize": 2,
  "workbench.editor.wrapTabs": true,
  "settingsSync.keybindingsPerPlatform": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.cursorBlinking": "solid",
  "terminal.integrated.env.osx": {},
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.cursorSurroundingLines": 8,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "visible",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font Mono",
  "editor.rulers": [],
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "git.openRepositoryInParentFolders": "never",

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "projectManager.tags": ["Personal", "Work", "Ewros"],

  "workbench.colorCustomizations": {
    "statusBar.background": "#56B6C2",
    "statusBar.noFolderBackground": "#56B6C2",
    "statusBar.debuggingBackground": "#56B6C2",
    "sash.hoverBorder": "#343944",
    "panel.border": "#3e4451",
    "editorGroup.border": "#3e4451",
    "sideBar.border": "#3e4451",
    "statusBar.foreground": "#ffffff",
    "statusBarItem.hoverBackground": "#343944",
    "statusBarItem.remoteBackground": "#1e2127",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "statusBar.border": "#282c34",
    "statusBar.debuggingBorder": "#343028",
    "commandCenter.border": "#e7e7e799",
    "titleBar.border": "#282c34",
    "titleBar.inactiveBackground": "#1e212799",
